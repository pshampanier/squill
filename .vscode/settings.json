{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.rulers": [120],
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "[rust]": {
    "editor.defaultFormatter": "jinxdash.prettier-rust",
    "editor.tabSize": 4
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.linkedProjects": ["./Cargo.toml"],
  "rust-analyzer.showUnlinkedFileNotification": false,
  "lldb.displayFormat": "auto",
  "lldb.showDisassembly": "auto",
  "lldb.dereferencePointers": true,
  "lldb.consoleMode": "commands",
  "cSpell.ignorePaths": ["**/node_modules/**", "**/target/**", "**/table-view.preview.tsx", ".vscode/**"],
  "cSpell.words": [
    "aliquam",
    "APPDATA",
    "appender",
    "Backquote",
    "Bearerxxxx",
    "blandit",
    "classix",
    "commandline",
    "cursus",
    "Datasource",
    "datasources",
    "datetime",
    "dbname",
    "dkey",
    "doctag",
    "duis",
    "dvalue",
    "eget",
    "elementum",
    "elit",
    "enim",
    "Fullcolor",
    "gravida",
    "hljs",
    "immer",
    "immerable",
    "imperdiet",
    "Keychain",
    "linenumber",
    "loaderfn",
    "malesuada",
    "mattis",
    "mcfly",
    "metatag",
    "morbi",
    "mydatabase",
    "mypassword",
    "myuser",
    "natoque",
    "neque",
    "nulla",
    "nunc",
    "oneshot",
    "pellentesque",
    "penatibus",
    "PGSQL",
    "porttitor",
    "RDBMS",
    "reqwest",
    "sagittis",
    "serde",
    "Shampanier",
    "skey",
    "sociis",
    "sqlx",
    "SQUILL",
    "sslmode",
    "sval",
    "svalue",
    "svgr",
    "sysinfo",
    "tailwindcss",
    "tauri",
    "tempdir",
    "tempfile",
    "titlebar",
    "ultrices",
    "unistd",
    "urlencoding",
    "vestibulum",
    "windicss",
    "xvalue",
    "zustand"
  ],
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "svg.preview.background": "transparent"
}
