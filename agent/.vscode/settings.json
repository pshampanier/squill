{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "prettier.printWidth": 120,
    "editor.rulers": [120],
    "[rust]": {
        "editor.defaultFormatter": "jinxdash.prettier-rust"
    },
    "rust-analyzer.checkOnSave": true,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.linkedProjects": ["./Cargo.toml"],
    "cSpell.words": [
        "aliquam",
        "APPDATA",
        "appender",
        "Bearerxxxx",
        "blandit",
        "commandline",
        "cursus",
        "Datasource",
        "datasources",
        "duis",
        "eget",
        "elementum",
        "elit",
        "enim",
        "gravida",
        "imperdiet",
        "malesuada",
        "mattis",
        "mcfly",
        "morbi",
        "natoque",
        "neque",
        "nulla",
        "nunc",
        "oneshot",
        "pellentesque",
        "penatibus",
        "porttitor",
        "reqwest",
        "sagittis",
        "serde",
        "sociis",
        "sqlx",
        "SQUILL",
        "sysinfo",
        "tempdir",
        "tempfile",
        "ultrices",
        "unistd",
        "vestibulum"
    ],
    "rust-analyzer.showUnlinkedFileNotification": false
